{
  "extends":"airbnb",
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "semi": [0],
    "import/no-extraneous-dependencies":[0],
    "react/jsx-filename-extension": [0],
    "import/extensions": [0]
  }
}
